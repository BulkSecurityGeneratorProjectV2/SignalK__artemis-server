{
   "config": {
      "hawtio": {
         "authenticationEnabled": false,
         "context": "/hawtio",
         "port": 8000,
         "start": false,
         "war": "./hawtio/hawtio-default-offline-1.4.48.war"
      },
      "jolokia": {
         "authenticationEnabled": false,
         "context": "/jolokia",
         "port": 8001,
         "war": "./hawtio/jolokia-war-1.3.3.war"
      },
      "server": {
         "apps": {
            "install": {
               "allow": true
            },
            "upgrade": {
               "allow": true
            }
         },
         "clock": {
            "src": "system"
         },
         "demo": {
            "file": "motu.log",
            "start": false
         },
         "files": {
            "maps": {
               "dir": "./mapcache/"
            },
            "static": {
               "dir": "./signalk-static/"
            },
            "storage": {
               "root": "./storage/"
            },
            "usb": {
               "usbdrive": "/media/usb0"
            }
         },
         "security": {
            "config": {
               "ip": ["127.0.0.1/8"]
            }
         },
         "serial": {
            "baud": 38400,
            "enable": true,
            "ports": ["/dev/ttyUSB0","/dev/ttyUSB1","/dev/ttyUSB2","/dev/ttyACM0","/dev/ttyACM1","/dev/ttyACM2"]
         },
         "server": {
            "mqtt": {
               "port": 1883,
               "start": true
            },
            "nmea": {
               "generate0183": true
            },
            "rest": {
               "port": 8080
            },
            "stomp": {
               "port": 61613,
               "start": true
            },
            "tcp": {
               "nmea": {
                  "port": 55557
               },
               "port": 55555
            },
            "udp": {
               "nmea": {
                  "port": 55556
               },
               "port": 55554
            },
            "websocket": {
               "port": 3000
            },
            "zeroconf": {
               "auto": true
            }
         },
         "version": "v1.0.0",
         "vessel": {
            "uuid": "urn:mrn:signalk:uuid:b2583c46-8f1a-486c-97ed-b99c33583e62"
         }
      }
   }
}
